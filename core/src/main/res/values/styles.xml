<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="CustomTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="fontFamily">@font/gilroy</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textFontWeight" tools:ignore="NewApi">500</item>
        <item name="android:textAllCaps">false</item>
        <item name="textAllCaps">false</item>
    </style>
    <style name="clickable">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:foreground">@drawable/ripple</item>
    </style>
    <style name="clickable_rounded">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:foreground">@drawable/ripple_rounded</item>
    </style>
</resources>
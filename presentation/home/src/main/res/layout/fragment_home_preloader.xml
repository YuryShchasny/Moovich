<?xml version="1.0" encoding="utf-8"?>
<com.sb.moovich.core.views.shimmer.ShimmerFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:shimmer_auto_start="true"
    tools:ignore="MissingClass">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="match_parent"
                android:layout_height="255dp"
                android:layout_marginBottom="47dp"
                android:background="@color/grey2"
                app:shimmer_auto_start="true"
                app:shimmer_base_alpha="1"
                app:shimmer_base_color="@color/grey2"
                app:shimmer_highlight_color="@color/grey" />

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="match_parent"
                android:layout_height="354dp"
                android:layout_marginBottom="59dp"
                android:background="@color/grey2"
                app:shimmer_auto_start="true"
                app:shimmer_base_alpha="1"
                app:shimmer_base_color="@color/grey2"
                app:shimmer_highlight_color="@color/grey" />

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="match_parent"
                android:layout_height="277dp"
                android:background="@color/grey2"
                app:shimmer_auto_start="true"
                app:shimmer_base_alpha="1"
                android:layout_marginBottom="56dp"
                app:shimmer_base_color="@color/grey2"
                app:shimmer_highlight_color="@color/grey" />

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="match_parent"
                android:layout_height="203dp"
                android:background="@color/grey2"
                app:shimmer_auto_start="true"
                app:shimmer_base_alpha="1"
                android:layout_marginBottom="56dp"
                app:shimmer_base_color="@color/grey2"
                app:shimmer_highlight_color="@color/grey" />

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="match_parent"
                android:layout_height="277dp"
                android:background="@color/grey2"
                app:shimmer_auto_start="true"
                app:shimmer_base_alpha="1"
                app:shimmer_base_color="@color/grey2"
                app:shimmer_highlight_color="@color/grey" />

        </LinearLayout>
    </ScrollView>

</com.sb.moovich.core.views.shimmer.ShimmerFrameLayout>